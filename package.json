{
  "name": "react-2048-game",
  "version": "0.1.0",
  "homepage": "https://devrsi0n.github.io/React-2048-game/",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "gitment": "^0.0.3",
    "lodash.debounce": "^4.0.8",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-undo": "^1.0.0-beta9-9-6",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "PRE_PUSH=true node scripts/test.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "analyze": "ANALYZE=true node scripts/build.js",
    "test": "node scripts/test.js",
    "ci": "CI=true node scripts/test.js",
    "codecov": "codecov",
    "predeploy": "node scripts/build.js",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "./coverage/",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.js",
      "<rootDir>/src/utils/registerServiceWorker.js",
      "<rootDir>/src/utils/mobileEvents.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.scss$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-2"
    ],
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "engines": {
    "node": ">= 6.10.0",
    "npm": ">= 4.5.0"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "ie > 9"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "no-plusplus": 0,
      "no-console": 0,
      "class-methods-use-this": 0,
      "no-restricted-syntax": 0
    }
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "global"
          ]
        }
      ],
      "max-nesting-depth": 2
    },
    "ignoreFiles": [
      "build/**/*.css"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.2.0",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "0.28.7",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "jest-localstorage-mock": "^2.0.1",
    "lint-staged": "^4.2.3",
    "node-sass": "^4.5.3",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.7.4",
    "react-dev-utils": "^4.1.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-manifest-plugin": "^1.2.1",
    "webpack-monitor": "^1.0.13"
  }
}
